#!/usr/bin/env ruby
# frozen_string_literal: true

File.new("#{__dir__}/../lib/ylem.rb").tap do |file|
  # noinspection RubyResolve
  require_relative file if File.file?(file)
end

require 'ylem/cli'

Ylem::Cli.call(progname: File.basename(__FILE__)).to_i.tap do |status_code|
  exit(status_code)
end
